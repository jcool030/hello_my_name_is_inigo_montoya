        </activity>
        <activity android:name="com.seg2105app.activities.SignUpScreenActivity" />
        <activity android:name="com.seg2105app.activities.homeowner.HomeOwnerWelcome" />
        <activity android:name="com.seg2105app.activities.admin.AdminWelcome" />
