        <activity android:name="com.seg2105app.activities.admin.CreateServiceActivity" />
        <activity android:name="com.seg2105app.activities.admin.ManageServiceActivity" />
        <activity android:name="com.seg2105app.activities.admin.ServiceEditorActivity" />
